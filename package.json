{
  "name": "streamline",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "sandbox:start": "yarn nx run sandbox:start",
    "sandbox:start:e2e": "NX_E2E=true yarn nx run sandbox:start-e2e",
    "sandbox:start:clear": "yarn nx run sandbox:start --clear",
    "sandbox:test": "yarn nx run streamline:test",
    "sandbox:test:visual:ios": "yarn nx run sandbox:test-visual-ios",
    "sandbox:test:visual:ios:compare": "yarn nx run sandbox:test-visual-ios-compare",
    "sandbox:test:visual:android": "yarn nx run sandbox:test-visual-android",
    "sandbox:test:visual:android:compare": "yarn nx run sandbox:test-visual-android-compare",
    "streamline:test": "yarn nx run streamline:test",
    "streamline:build": "yarn nx build streamline",
    "streamline:publish": "cd dist/packages/streamline && npm publish",
    "streamline:generate:component": "yarn nx g @nx/react-native:component $C --project=streamline --export --directory components",
    "streamline:generate:icons:regular": "yarn nx run streamline:icons:generate:regular",
    "streamline:generate:icons:small": "yarn nx run streamline:icons:generate:small",
    "lint": "yarn nx run-many --target=lint --ext '.js,.jsx,.ts,.tsx' --all --parallel",
    "test": "yarn nx run-many --target=test --all --parallel",
    "type:check": "yarn nx run-many --target=type:check --all --parallel",
    "format": "yarn nx format:write --all"
  },
  "private": true,
  "dependencies": {
    "@expo/metro-config": "0.7.1",
    "@expo/webpack-config": "18.0.4",
